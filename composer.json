{
    "name": "allnetru/sharding",
    "description": "Laravel database sharding toolkit extracted from Boost",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "allnetru",
            "email": "dev@allnet.ru"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/pagination": "^10.0|^11.0|^12.0",
        "illuminate/redis": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Allnetru\\Sharding\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Allnetru\\Sharding\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Allnetru\\Sharding\\Providers\\ShardingServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
